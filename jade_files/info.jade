  #shower-section
    .heading When and Where?
    #location
      address
        p 2454 Pine Hollow Dr.
        p East Lansing, MI 48823
        p Saturday July 30, 2016
        p 4:30pm - 7:00pm 
    #rsvp
      form(role="form" name="rsvp" method="POST")
        input.rsvp-form#guest_name(name="guest_name" type="name" placeholder="Name")
        input.rsvp-form#attending(name="attending" type="text" placeholder="Attending: Yes or No?")
        input.rsvp-form#guest(name="guest" type="number" placeholder="How Many?")
        input.btn-submit#rsvp-submit(type="submit" value="RSVP")
  #last-row.flex-center

    #registry-info-main
      p.heading Registry Info
